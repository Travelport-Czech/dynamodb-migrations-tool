{
  "name": "dynamit-cli",
  "version": "1.2.5",
  "description": "The DynamoDB migrations tool CLI",
  "bin": {
    "dynamit": "./dist/dynamit",
    "dynamit-cli": "./dist/dynamit"
  },
  "scripts": {
    "build": "npm run build-clean && tsc && npm run build-bin && npm run build-assets",
    "build-bin": "mv ./dist/index.js ./dist/dynamit && chmod +x ./dist/dynamit",
    "build-assets": "cp -R .plop/. ./dist/.plop/",
    "build-clean": "rm -rf ./dist/",
    "lint": "eslint . --ext .ts --ignore-path .gitignore",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/floydspace/dynamodb-migrations-tool.git"
  },
  "keywords": [
    "cli",
    "umzug",
    "dynamodb",
    "aws",
    "serverless",
    "sequelize",
    "migrate",
    "migration",
    "migrations"
  ],
  "author": "Victor Korzunin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/floydspace/dynamodb-migrations-tool/issues"
  },
  "homepage": "https://floydspace.github.io",
  "devDependencies": {
    "@types/cli-color": "^2.0.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.2",
    "@types/umzug": "^2.2.3",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "aws-sdk-mock": "^5.1.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "ts-jest": "^25.5.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "aws-sdk": "^2.673.0",
    "cli-color": "^2.0.0",
    "node-plop": "^0.25.0",
    "umzug": "^2.3.0",
    "yargs": "^15.3.1"
  }
}
